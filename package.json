{
  "name": "mern-starter",
  "version": "1.0.0",
  "description": "Boilerplate stater package for MERN stack applications",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "node.js",
    "express"
  ],
  "author": "Chaitanya Karanam",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "mongoose": "^5.0.0-rc0"
  },
  "devDependencies": {
    "nodemon": "^1.14.7"
  }
}
